{
    "name": "norman-huth/nova-hide-navigation",
    "description": "Hiding Sidebar, Header or Footer in Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "nova-assets",
        "nova-navigation"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "NormanHuth\\NovaHideNavigation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NormanHuth\\NovaHideNavigation\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
